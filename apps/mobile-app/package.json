{
  "name": "@mobouzer/mobile-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo start --dev-client --android",
    "ios": "expo start --dev-client --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build": "tsc --noEmit -p . --pretty",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write .",
    "postinstall": "node ./bin/postInstall",
    "recrawl": "watchman watch-del ./; watchman watch-project ./",
    "eas-build-pre-install": "echo $GOOGLE_SERVICES_ANDROID_BASE64 | base64 --decode > ./google-services.json && echo $GOOGLE_SERVICES_IOS_BASE64 | base64 --decode > ./GoogleService-Info.plist",
    "eas-build-post-install": "pnpm run -r --if-present build"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.2",
    "@expo-google-fonts/space-mono": "^0.2.2",
    "@expo/vector-icons": "^13.0.0",
    "@gorhom/bottom-sheet": "^4",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-firebase/app": "14.11.1",
    "@react-native-firebase/auth": "14.11.1",
    "@react-native-firebase/firestore": "14.11.1",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@skillnation/react-native-firebase-hooks": "^0.5.0",
    "dotenv": "^16.0.1",
    "expo": "^45.0.0",
    "expo-application": "~4.1.0",
    "expo-asset": "~8.5.0",
    "expo-auth-session": "~3.6.1",
    "expo-checkbox": "~2.1.0",
    "expo-constants": "~13.1.1",
    "expo-dev-client": "~1.0.1",
    "expo-font": "~10.1.0",
    "expo-linking": "~3.1.0",
    "expo-random": "~12.2.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "expo-system-ui": "~1.2.0",
    "expo-updates": "~0.13.3",
    "expo-web-browser": "~10.2.1",
    "firebase": "~9.7.0",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "moti": "^0.18.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-firebase-hooks": "^5.0.3",
    "react-native": "0.68.2",
    "react-native-clusterer": "^1.2.0",
    "react-native-gesture-handler": "~2.2.1",
    "react-native-maps": "0.30.2",
    "react-native-reanimated": "~2.8.0",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-web": "0.17.7",
    "swr": "^1.2.2",
    "twrnc": "^3.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-typescript": "^7.18.6",
    "@expo/config-types": "^45.0.0",
    "@types/lodash": "^4.14.182",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.67.6",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jetifier": "^2.0.0",
    "prettier": "^2.7.1",
    "react-test-renderer": "17.0.1",
    "typescript": "^4.7.4"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "no-unused-vars": "off",
      "arrow-parens": "off",
      "no-console": "warn",
      "prettier/prettier": [
        "warn"
      ],
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/camelcase": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "destructuredArrayIgnorePattern": "^_"
        }
      ],
      "@typescript-eslint/no-var-requires": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-non-null-assertion": "off",
      "@typescript-eslint/no-namespace": "off",
      "jsx-a11y/no-noninteractive-tabindex": 0,
      "jsx-a11y/anchor-is-valid": "off",
      "jsx-a11y/no-onchange": "off",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn",
      "react/react-in-jsx-scope": "off",
      "react/no-unescaped-entities": "off"
    },
    "overrides": [
      {
        "files": [
          "**/*.tsx"
        ],
        "rules": {
          "react/prop-types": "off"
        }
      }
    ],
    "plugins": [
      "@typescript-eslint",
      "prettier",
      "jsx-a11y",
      "react",
      "react-hooks"
    ],
    "settings": {
      "react": {
        "pragma": "React",
        "version": "17.0"
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    }
  }
}
